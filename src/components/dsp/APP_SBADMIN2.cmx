<DAT name="SFUNC" xml:space='preserve'>#define CREATE
;----------------------------------------------------------------
;
;----------------------------------------------------------------
operation create
;----------------------------------------------------------------
params
	Struct pOptions : INOUT
endparams

variables
	Struct  vRoutes
	String  vResourceName
	String  vAction
	String  vParam
	String  vMenuMapJson
	Numeric i
	Numeric vSize
	String  vAlias
	Struct  vResource
	String  vDspName
	Struct  vOptions
endvariables

	vRoutes = pOptions-&gt;"routes"

	vResourceName = pOptions-&gt;"resource"
	vAction       = pOptions-&gt;"action"
	vParam        = pOptions-&gt;"param"

	USERNAME.STATUS = lpGetUserName()

	if (vRoutes = "")
		vDspName = $uppercase(vResourceName)
	else
		;Figure out what DSP we should be displaying, look up the passed page in the 
		;routes struct.
		vResource = lpGetResourceByNameAndAction(vResourceName, vAction, vRoutes)
		vDspName = vResource-&gt;"dsp"
	endif

	if(vDspName == "")
		;Default the DSP to the first route
		vResourceName = vRoutes-&gt;*{1}-&gt;$name

		;Update the options so that they're picked up in createJs
		pOptions-&gt;"resource" = vResourceName
		pOptions-&gt;"action"   = ""
		pOptions-&gt;"param"    = ""
	endif

	return 0

end ;create


#define CREATE_JS
;----------------------------------------------------------------
;
;----------------------------------------------------------------
weboperation createJS
;----------------------------------------------------------------
params
	String pOptionsJson : IN
endparams

javascript

	//Register this DSP as the application's menu
	app.menuInstance = this;

	try {
		var options = JSON.parse(pOptionsJson);
	} catch(e) {
		console.error("&lt;$componentname&gt;: Parse error");
	}

	//Setup the loading indicator
	var opts = {
		lines: 17,            // The number of lines to draw
		length: 18,           // The length of each line
		width: 4,             // The line thickness
		radius: 20,           // The radius of the inner circle
		corners: 1,           // Corner roundness (0..1)
		rotate: 0,            // The rotation offset
		direction: 1,         // 1: clockwise, -1: counterclockwise
		color: '#333333',     // #rgb or #rrggbb or array of colors
		speed: 0.75,          // Rounds per second
		trail: 90,            // Afterglow percentage
		shadow: false,        // Whether to render a shadow
		hwaccel: false,       // Whether to use hardware acceleration
		className: 'spinner', // The CSS class to assign to the spinner
		zIndex: 2e9,          // The z-index (defaults to 2000000000)
		top: '200px',         // Top position relative to parent in px
		left: 'auto'          // Left position relative to parent in px
	};
	this.app.spinner = new Spinner(opts);


	/*
	 * navigationShowScreen
	 *
	 * Navigation specific code to run when changing a page
	 *
	 * TODO: Some sort of registration mechanism would work better
	 *       here, rather than one big switch.
	 */
	this.app.navigationShowScreen = function(field) {

		var occ = field.getParent();
		#for TAB = (&lt;TABS&gt;)
		occ.getField("&lt;TAB&gt;").setProperty("class:active", "false")
		#endfor

		var fieldName = field.getName();

		field.setProperty("class:active", "true");
	}

	/*
	 *  Run any navigation specific init code
	 */
	this.activate("navigationInit");

	app.showScreen(options.resource, options.action, options.param);

endjavascript

end ;createJS


;----------------------------------------------------------------
; Navigation specific code to run when changing a page
;----------------------------------------------------------------
weboperation navigationShowScreen
;----------------------------------------------------------------
params
	String pResource : IN
endparams

javascript

	var vOccs,
	len,
	vOcc;

	//Hide all the page divs and show the loading indicator
	vOccs = this.getEntity("DUMMY.DUMMY").getOccurrences();
	len = vOccs.length;

	for(var i = 0; i &lt; len; i++) {

		vOcc = vOccs[i];
		vOcc.setProperty("style:display", "none");
	}

	if(typeof pResource === "undefined" || pResource == "") {
		pResource = "about";
	}

	//Turn on the loading indicator
	var target = document.querySelector('.container');
	this.app.spinner.spin(target);

	var occ = this.getEntity("MENU.DUMMY").getOccurrence(0);
	#for TAB = (&lt;TABS&gt;)
	occ.getField("&lt;TAB&gt;").setProperty("class:active", "false");
	#endfor

	var field = occ.getField(pResource);
	field.setProperty("class:active", "true");

endjavascript

end ;navigationShowScreen


;----------------------------------------------------------------
; Navigation specific code to run when setting up the menu
; for the first time.
;
; TODO: Some sort of registration mechanism would work better
;       here, rather than one big switch.
;----------------------------------------------------------------
weboperation navigationInit
;----------------------------------------------------------------

javascript

	$('#side-menu').metisMenu();

endjavascript

end ;navigationInit


;----------------------------------------------------------------
; Navigation specific code to run having just created or setup
; a DSP (i.e. the user is navigating to a new or existing DSP)
;----------------------------------------------------------------
weboperation postDspSetup
;----------------------------------------------------------------
params
	String pResourceName : IN
	String pInstanceName : IN
endparams

javascript

	var vOccs,
	len,
	vOcc;

	//Show the page div we're interested in
	vOccs = this.getEntity("DUMMY.DUMMY").getOccurrences();
	len = vOccs.length;

	for(var i = 0; i &lt; len; i++) {

		vOcc = vOccs[i];

		if(vOcc.getField("PAGE").getValue() == pInstanceName) {
			//We're currently on this page, so show it
			vOcc.setProperty("style:display", "block");
		}
	}
	
	//Turn off the loading spinner to reveal our page
	this.app.spinner.stop();

endjavascript

end ;postDspSetup



;----------------------------------------------------------------
; Reset the page. We don't need anything from the server so
; this can be done with a weboperation. We might even be logged
; out at this point so going back to the server could trigger
; a not authorised error.
;----------------------------------------------------------------
weboperation detach
;----------------------------------------------------------------

javascript

	this.getEntity("DUMMY.DUMMY").clear();

endjavascript

end ;detach


